{
	"name" : "dirtsimple/fun-factory",
	"type" : "library",
	"description" : "Fluently and efficiently compose functions, methods, and lambda-expression strings",
	"license" : "MIT",
	"require" : {
		"php" : ">=5.6"
	},
	"keywords" : [
		"functional programming",
		"partial application",
		"fluent interfaces",
		"function composition"
	],
	"autoload" : {
		"psr-4" : {
			"dirtsimple\\" : "src"
		},
		"files" : [
			"src/functions.php"
		]
	},
	"require-dev" : {
		"peridot-php/leo" : "~1.6",
		"peridot-php/peridot" : "~1.19",
		"brain/monkey" : "~2.2",
		"peridot-php/peridot-watcher-plugin": "^1.3"
	},
	"config" : {
		"process-timeout": 0
	},
	"scripts" : {
		"watch": "peridot --force-colors --watch specs",
		"test" : "peridot specs"
	}
}